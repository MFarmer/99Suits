<h1>Profile Settings</h1>

<%= image_tag current_user.avatar.url(:thumb) %>

<%= form_for current_user, :url => user_url(current_user.id) do |f| %>

  <%= f.label :avatar %><br>
  <%= f.file_field :avatar %><br><br>

  <%= f.label :fname %><br>
  <%= f.text_field :fname %><br><br>

  <%= f.label :lname %><br>
  <%= f.text_field :lname %><br><br>

  <%= f.label :email %><br>
  <%= f.text_field :email %><br><br>

  <%= f.label :username %><br>
  <%= f.text_field :username %><br><br>

<% end %>